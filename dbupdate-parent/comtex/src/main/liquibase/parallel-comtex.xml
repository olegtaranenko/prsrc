<?xml version="1.0" encoding="cp1251"?>
 
<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd"
  logicalFilePath="parallel-comtex.xml"
>

         

	<changeSet id="1" author="parallel" runOnChange="true" context="parallel">
		<comment>Научиться запускать и работать параллельно в две версиях системы: production и development</comment>
		<update tableName="guides">
			<column name="namer" value="${this.server.name}"/>
			<where>id = 0</where>
		</update>
		<sql><![CDATA[alter server prior class 'ASAODBC' USING 'DSN=dev_prior;UID=dba;PWD=sql']]></sql>
	</changeSet>

</databaseChangeLog>

